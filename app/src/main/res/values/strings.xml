    <!-- Quick mobility test -->
    <string name="hip_rotation_test">Hip rotation test</string>
    <string name="hip_rotation_test_description">Sit on the floor with your legs extended in front of you. Slowly rotate your leg inward and then outward while keeping your torso straight.</string>
    <string name="hip_rotation_test_response_easy">Full range of motion with ease</string>
    <string name="hip_rotation_test_response_medium">Some difficulty rotating</string>
    <string name="hip_rotation_test_response_hard">Unable to rotate fully</string>
    <string name="forward_bend_test">Forward Bend Test</string>
    <string name="forward_bend_test_description">Stand with your feet together and slowly bend forward at the hips, trying to touch your toes without bending your knees.</string>
    <string name="forward_bend_test_response_easy">Can easily touch my toes</string>
    <string name="forward_bend_test_response_medium">Can reach just above my toes</string>
    <string name="forward_bend_test_response_hard">Cannot reach my toes</string>
    <string name="behind_the_back_reach_test">Behind-the-Back reach test</string>
    <string name="behind_the_back_reach_test_description">Reach one hand over your shoulder and the other hand behind your back, trying to make your hands touch.</string>
    <string name="behind_the_back_reach_test_response_easy">Hands can easily touch</string>
    <string name="behind_the_back_reach_test_response_medium">Hands are close but not touching</string>
    <string name="behind_the_back_reach_test_response_hard">Hands are far apart</string>
    <string name="wall_extension_test">Wall Extension Test</string>
    <string name="wall_extension_test_description">Stand with your heels, back, and head against a wall. Try to lift your arms overhead without moving your head or back away from the wall.</string>
    <string name="wall_extension_test_response_easy">Can fully extend arms without losing contact</string>
    <string name="wall_extension_test_response_medium">Some difficulty maintaining contact</string>
    <string name="wall_extension_test_response_hard">Cannot fully extend arms without losing contact</string>
    <string name="quick_mobility_test_no_recommendation">Based on your results, we haven\'t been able to determine a clear recommendation since you scored the same on three or more exercises, feel free to choose the objective that best suits your preferences.</string>
